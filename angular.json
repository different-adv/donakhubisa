{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "donakhubisa": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/donakhubisa",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              // "src/styles.css",
              // "node_modules/bootstrap/dist/css/bootstrap.min.css",
              // "node_modules/font-awesome/css/font-awesome.css",
              // "src/lib/css/sb-admin-2.min.css",
              // "src/lib/css/animate.min.css",
              // "src/lib/css/bracket.css",
              // "src/lib/css/bracket.min.css",
              // "src/lib/css/fluidbox.min.css",
              // "src/lib/css/icon.css",
              // "src/lib/css/idangerous.swiper.css",
              // "src/lib/css/loader.css",
              // "src/lib/css/style.css"
              "src/assets/css/bootstrap/bootstrap-grid.css",
              "src/assets/css/bootstrap/bootstrap-reboot.css",
              "src/assets/css/animate.css",
              "src/assets/css/aos.css",
              "src/assets/css/bootstrap-datepicker.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/flaticon.css",
              "src/assets/css/icomoon.css",
              "src/assets/css/ionicons.min.css",
              "src/assets/css/jquery.timepicker.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/open-iconic-bootstrap.min.css",
              // "src/assets/css/owl.carousel.min.css",
              // "src/assets/css/owl.theme.default.min.css",
              "src/assets/css/style.css",
              "src/assets/css/css/mixins/_text-hide.css",

              // charity
              "src/assets/CharityPosts Styles/css/style.css",
              // "src/assets/CharityPosts Styles/css/bootstrap.min.css",
              // "src/assets/CharityPosts Styles/css/font-awesome.min.css",
              "src/assets/CharityPosts Styles/css/owl.carousel.css",
              "src/assets/CharityPosts Styles/css/owl.theme.default.css"


              // //profile
              // "src/assets/ProfileStyles/css/style1.css",
              // "src/assets/ProfileStyles/css/icomoon.css",
              // "src/assets/ProfileStyles/css/flexslider.css",
              // "src/assets/ProfileStyles/css/animate1.css"
              








            ],
            "scripts": [
              // "node_modules/bootstrap/dist/js/bootstrap.min.js",
              // "node_modules/jquery/dist/jquery.min.js",
              // "src/lib/js/bracket.js",
              // "src/lib/js/script.js"

              



              "src/assets/js/aos.js",
              "src/assets/js/bootstrap-datepicker.js",
              "src/assets/js/google-map.js",
              "src/assets/js/jquery-3.2.1.min.js",
              "src/assets/js/jquery-migrate-3.0.1.min.js",
              "src/assets/js/jquery.animateNumber.min.js",
              "src/assets/js/jquery.easing.1.3.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/jquery.min.js",
              "src/assets/js/jquery.stellar.min.js",
              "src/assets/js/jquery.timepicker.min.js",
              "src/assets/js/jquery.waypoints.min.js",
              "src/assets/js/main.js",
              // "src/assets/js/owl.carousel.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/range.js",
              "src/assets/js/scrollax.min.js",


              // charity
              "src/assets/CharityPosts Styles/js/main.js"

              // profile
              // "src/assets/ProfileStyles/js/respond.min.js",
              // "src/assets/ProfileStyles/js/modernizr-2.6.2.min.js",
              // "src/assets/ProfileStyles/js/main1.js",
              // "src/assets/ProfileStyles/js/jquery.flexslider-min.js"
              














            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "donakhubisa:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "donakhubisa:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "donakhubisa:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "donakhubisa:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "donakhubisa:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "donakhubisa"
}
